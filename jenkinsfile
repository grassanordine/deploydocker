node {
  
   stage('SCM Checkout') {
      git 'https://github.com/grassanordine/deploydocker'     
          
   }

   stage('Build Package') {
      def mvnHome = tool name: '3.0.6', type: 'maven'

      sh "${mvnHome}/bin/mvn package"
   }

   stage('Test image') {
         
          sh 'echo "Tests passed"'
      
   }

  
}
